void divisors(int k)
{
	for(int i = 1 ; i <= sqrt(k) ; i++)
	{
		if(k % i == 0)
		{
			if(k / i != i)
			{
				//Do something
			}
			else
			{
				//Do something
			}
		}
	}
}
]]></content>
